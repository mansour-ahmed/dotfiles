{
  "markdown.extension.preview.autoShowPreviewToSide": false,
  "window.zoomLevel": 2,
  "git.enableSmartCommit": true,
  "editor.wordWrap": "off",
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/_build": true,
    "**/_deploy": true,
    "**/_site": true,
    "**/.bundle": true,
    "**/.coverage": true,
    "**/.DS_Store": true,
    "**/.expo": true,
    "**/.git": true,
    "**/.githooks": true,
    "**/.hg": true,
    "**/.pygments-cache": true,
    "**/.sass-cache": true,
    "**/.svn": true,
    "**/.tox": true,
    "**/.vagrant": true,
    "**/.venv": true,
    "**/.virtualenv": true,
    "**/*.a": true,
    "**/*.class": true,
    "**/*.db": true,
    "**/*.dll": true,
    "**/*.dylib": true,
    "**/*.egg-info": true,
    "**/*.exe": true,
    "**/*.idb": true,
    "**/*.lib": true,
    "**/*.ncb": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/*.pdb": true,
    "**/*.psd": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.sdf": true,
    "**/*.so": true,
    "**/*.sublime-workspace": true,
    "**/*.suo": true,
    "**/backstop_data": true,
    "**/build": true,
    "**/coverage": true,
    "**/css_regression_data": true,
    "**/CVS": true,
    "**/db.sqlite3": true,
    "**/env": true,
    "**/es5": true,
    "**/npm-debug.log*": true,
    "**/npm-shrinkwrap.json": true,
    "**/static/**/*.css": false,
    "**/static/**/*.js": false,
    "**/static/wagtailadmin": true,
    "**/tmp": true,
    "**/var": true,
    "**/venv": true,
    "**/www": true
  },
  "window.reopenFolders": "all",
  "terminal.external.osxExec": "iTerm.app",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "markdownlint.config": {
    "MD003": false,
    "MD013": false,
    "MD024": false
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.openPositioning": "right",
  "workbench.sideBar.location": "left",
  "html.format.wrapLineLength": 0,
  "html.format.extraLiners": "object",
  "html.suggest.angular1": false,
  "html.suggest.ionic": false,
  "editor.tabSize": 4,
  "git.enabled": true,
  "git.autorefresh": true,
  "git.autofetch": true,
  "git.countBadge": "all",
  "css.validate": true,
  "scss.validate": true,
  "jsAutolint.defaultLinters": ["eslint", "standard"],
  "jsAutolint.enable": true,
  "typescript.check.npmIsInstalled": false,
  "editor.fontSize": 13,
  "tslint.jsEnable": true,
  "prettier.singleQuote": true,
  "javascript.format.enable": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.preview.linkify": false,
  "markdown.preview.markEditorSelection": false,
  "eslint.packageManager": "yarn",
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.format.defaultFormatter.ts": "prettier-tslint",
  "search.useIgnoreFiles": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Winter is Coming (Dark Black)",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false
}
